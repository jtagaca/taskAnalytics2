<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="298.0" prefWidth="452.0" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jtagaca.taskanalytics_.LoginController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <children>
        <TextField fx:id="txtUserName" onAction="#onLoginClicked" promptText="User Name" />
        <PasswordField fx:id="txtPassword"  promptText="Password" />
        <VBox fx:id="vboxMessage" alignment="CENTER">
            <children>
                <Label text="Use 3-10 Characters" textFill="RED" />
                <Label text="Alphanumeric Only" textFill="RED" />
            </children>
        </VBox>
        <Button id="btnRegister" fx:id="btnLogin" mnemonicParsing="false" onAction="#onLoginClicked" text="Register" />
      <Button id="btnLogin" fx:id="btnLogin1" layoutX="209.0" layoutY="195.0" mnemonicParsing="false" onAction="#onLoginClicked" text="Login" />
    </children>
</VBox>
